<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <ImageView
        android:id="@+id/tearsToManyMothers"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="63dp"
        android:contentDescription="@string/_1066_automalauncherimg"
        android:scaleType="fitCenter"
        android:scaleX="0.9"
        app:layout_constraintBottom_toTopOf="@+id/greatWesternTrail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tearstomanymothers" />

    <ImageView
        android:id="@+id/greatWesternTrail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:scaleType="fitCenter"
        android:scaleX="0.9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tearsToManyMothers"
        app:srcCompat="@drawable/gwt"
        android:contentDescription="@string/gwt_automalauncherimg"/>


</androidx.constraintlayout.widget.ConstraintLayout>